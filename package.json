{
  "name": "butter-vue3-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:stylelint": "stylelint . \"**/*.{vue,scss}\"",
    "lint:stylelint:fix": "stylelint . --fix \"**/*.{vue,less,postcss,css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@unocss/preset-rem-to-px": "^0.53.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mrm": "^4.1.14",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "sass": "^1.62.1",
    "stylelint": "^15.7.0",
    "stylelint-config-recess-order": "^4.1.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.2",
    "unocss": "^0.52.7",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "npm run lint",
    "*.{css,scss,vue}": "npm run lint:stylelint"
